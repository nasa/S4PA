<?xml version="1.0" encoding="UTF-8"?>
<!-- S4paDescriptor.xsd,v 1.76 2015/08/28 19:03:58 glei Exp -->
<!-- -@@@ S4PA, Version $Name:  $ -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="skipPublicationType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="OPERATOR" use="optional" default="EQ">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="EQ"/>
							<xs:enumeration value="NE"/>
							<xs:enumeration value="GT"/>
							<xs:enumeration value="GE"/>
							<xs:enumeration value="LT"/>
							<xs:enumeration value="LE"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="VALUE" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Value of the xpath for granule to be skipped. If not defined, the existence of the xpath will be used.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ignoreConditionType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="OPERATOR" use="optional" default="EQ">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="EQ"/>
							<xs:enumeration value="NE"/>
							<xs:enumeration value="GT"/>
							<xs:enumeration value="GE"/>
							<xs:enumeration value="LT"/>
							<xs:enumeration value="LE"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="uniqueAttributeType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="OPERATOR" use="optional" default="EQ">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="EQ"/>
							<xs:enumeration value="NE"/>
							<xs:enumeration value="GT"/>
							<xs:enumeration value="GE"/>
							<xs:enumeration value="LT"/>
							<xs:enumeration value="LE"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="dataPollerJobType">
		<xs:sequence>
			<xs:element name="dataset" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Dataset characteristics. The text content must be the pattern for matching filenames found during polling.</xs:documentation>
				</xs:annotation>
				<xs:complexType mixed="true">
					<xs:sequence>
						<xs:element name="file" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:attribute name="PATTERN" type="xs:string"  use="required">
									<xs:annotation>
										<xs:documentation>Pattern for primary file name</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="ALIAS" type="xs:string"  use="optional">
									<xs:annotation>
										<xs:documentation>Pattern for renaming the polled file</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="associateFile" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="PATTERN" type="xs:string"  use="required">
									<xs:annotation>
										<xs:documentation>Pattern for associated file name</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="ALIAS" type="xs:string"  use="optional">
									<xs:annotation>
										<xs:documentation>Pattern for renaming the polled file</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="NAME" use="required">
						<xs:annotation>
							<xs:documentation>Dataset name.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="([a-z]|[A-Z]|[0-9]|_|-){1,30}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="VERSION" use="optional">
						<xs:annotation>
							<xs:documentation>Dataset version</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\s*|\S{1,10}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="ALIAS" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Pattern for renaming the polled files</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="NAME" use="required">
			<xs:annotation>
				<xs:documentation>Name of the poller. Must be a alpha-numberical string.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="([a-z]|[A-Z]|[0-9]|_)+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="HOST" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Name of the host being polled.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DIR" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Name of the directory being polled.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PROTOCOL" use="optional">
			<xs:annotation>
				<xs:documentation>Protocol to be used for polling, default to FTP.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="FTP"/>
					<xs:enumeration value="FILE"/>
					<xs:enumeration value="HTTP"/>
					<xs:enumeration value="SFTP"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="PORT" use="optional">
			<xs:annotation>
				<xs:documentation>Port number to be used for SFTP polling, default to 22.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EXTERNAL_API" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>An API to supply the list of files (in URL|size format) for a http polling.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RECURSIVE" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Boolean value (true or false) indicating whether the polling is recursive for FTP polling.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAX_DEPTH" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum directory depth for FILE and HTTP polling.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IGNORE_HISTORY" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Boolean value (true or false) indicating whether the polling is ignoring history file checking or not, default to false.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAX_FILE_GROUP" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum file groups in a PDR (output of polling).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="REPOLL_PAUSE" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Sleep time in second before repolling to check if file size changed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MINIMUM_FILE_SIZE" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Minimum file size in bytes for file to be polled.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ORIGINATING_SYSTEM">
			<xs:annotation>
				<xs:documentation>Orginating system for use in PDRs. It's value is used to determine the remote location for pushing PANs.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="([a-z]|[A-Z]|[0-9]|_)+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="SUB_DIR_PATTERN" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Sub-directory name pattern in Linux date format for a ftp polling to limit the scanning only under matched directories.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LATENCY" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Latency in days before current date for a matching directory name pattern ftp polling.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="pdrPollerJobType">
		<xs:sequence>
			<xs:element name="exclude" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="dataset" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="NAME" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:pattern value="([a-z]|[A-Z]|[0-9]|_|-){1,30}"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="VERSION" use="optional">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:pattern value="\s*|\S{1,10}"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="pdrFilter" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="PATTERN" type="xs:string" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="NAME" use="required">
			<xs:annotation>
				<xs:documentation>Name of the poller. Must be alpha-numberical word.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="([a-z]|[A-Z]|[0-9]|_)+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="HOST" use="required">
			<xs:annotation>
				<xs:documentation>Name of the host being polled.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="\S+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="DIR" use="required">
			<xs:annotation>
				<xs:documentation>Name of the directory being polled.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="\S+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="IGNORE_HISTORY" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Boolean value (true or false) indicating whether the polling is ignoring history file checking or not, default to false.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MERGE_PAN" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Boolean value (true or false) indicating whether pan merging is required, default to false.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PATTERN" type="xs:string" use="optional" default="\.PDR$"/>
		<xs:attribute name="TYPE" use="optional" default="DEFAULT">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="EDOS"/>
					<xs:enumeration value="DEFAULT"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="PROTOCOL" use="optional">
			<xs:annotation>
				<xs:documentation>Protocol to be used for polling, default to FTP.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="FTP"/>
					<xs:enumeration value="FILE"/>
					<xs:enumeration value="SFTP"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="PORT" use="optional">
			<xs:annotation>
				<xs:documentation>Port number to be used for SFTP polling, default to 22.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RECURSIVE" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Boolean value (true or false) indicating whether the polling is recursive.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MAX_DEPTH" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum directory depth for FILE polling.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="dataclassType">
		<xs:sequence>
			<xs:element name="method" type="methodType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Methods applicable to entire data class.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="skipPublication" type="skipPublicationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>XPATH expressions that specify cases where granules does not need to be published.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataset" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Dataset information</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="method" type="methodType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Methods applicable to dataset.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ignoreCondition" type="ignoreConditionType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>XPATH expressions that specify cases where an incoming granule has to be ignored.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="uniqueAttribute" type="uniqueAttributeType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>XPATH expressions for determining the uniqueness of a granule. If the value of XPATH expression matches in all cases, incoming granule is deemed a valid replacement. Otherwise, it is treated as a new granule.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="associateData" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Associated dataset name and/or version, ex. the associated browse dataset.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="NAME" type="xs:string" use="required"/>
								<xs:attribute name="VERSION" type="xs:string" use="optional"/>
								<xs:attribute name="TYPE" type="xs:string" use="optional" default="Browse"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="postStorageTask" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Special task to be executed after each granule storage. S4PA will append filenames to the argument during command invokation. Can be use to fulfill any extra task to be done between granule ingest and publishing.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="postDeletionTask" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Special task to be executed after each granule deletion. S4PA will append filenames to the argument during command invokation. Can be use to fulfill any extra task to be done right before granule's phsical removal or clean up what was created by postStorageTask.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="skipPublication" type="skipPublicationType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>XPATH expressions that specify cases where granules does not need to be published.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="dataVersion" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Dataset version. LABEL can be an empty string for version-less system or a non-white space string for a versioned system.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="uniqueAttribute" type="uniqueAttributeType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>XPATH expressions for determining the uniqueness of a granule. If the value of XPATH expression matches in all cases, incoming granule is deemed a valid replacement. Otherwise, it is treated as a new granule.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ignoreCondition" type="ignoreConditionType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>XPATH expressions which specify conditions in which an incoming granule has to be ignored. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="associateData" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Associated dataset name and/or version, ex. the associated browse dataset.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attribute name="NAME" type="xs:string" use="required"/>
											<xs:attribute name="VERSION" type="xs:string" use="optional"/>
											<xs:attribute name="TYPE" type="xs:string" use="optional" default="Browse"/>
										</xs:complexType>
									</xs:element>
									<xs:element name="postStorageTask" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Special task to be executed after each granule storage. S4PA will append filenames to the argument during command invokation. Can be use to fulfill any extra task to be done between granule ingest and publishing.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="postDeletionTask" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Special task to be executed after each granule deletion. S4PA will append filenames to the argument during command invokation. Can be use to fulfill any extra task to be done right before granule's phsical removal or clean up what was created by postStorageTask.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="skipPublication" type="skipPublicationType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>XPATH expressions that specify cases where granules does not need to be published.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="LABEL" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:pattern value="\s*|\S{1,10}"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="COLLECTION_VERSION" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation>CMR Collection version. Must be specified for versionless dataset.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="ACCESS" use="optional" default="public">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="public"/>
											<xs:enumeration value="restricted"/>
											<xs:enumeration value="hidden"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="FREQUENCY" use="optional" default="daily">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="daily"/>
											<xs:enumeration value="monthly"/>
											<xs:enumeration value="yearly"/>
											<xs:enumeration value="none"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="TIME_MARGIN" type="xs:long" use="optional" default="0"/>
								<xs:attribute name="DIF_ENTRY_ID" type="xs:string" use="optional" default=""/>
								<xs:attribute name="PUBLISH_WHOM" type="xs:boolean" use="optional" default="false"/>
								<xs:attribute name="PUBLISH_ECHO" type="xs:boolean" use="optional" default="false"/>
								<xs:attribute name="PUBLISH_ECHO_OPENDAP" type="xs:boolean" use="optional"/>
								<xs:attribute name="PUBLISH_CMR_OPENDAP" type="xs:boolean" use="optional"/>
								<xs:attribute name="OPENDAP_URL_PREFIX" type="xs:string" use="optional"/>
								<xs:attribute name="OPENDAP_URL_SUFFIX" type="xs:string" use="optional"/>
								<xs:attribute name="OPENDAP_RESOURCE_URL_SUFFIX" type="xs:string" use="optional"/>
								<xs:attribute name="PUBLISH_CMR" type="xs:boolean" use="optional" default="true"/>
								<xs:attribute name="PUBLISH_MIRADOR" type="xs:boolean" use="optional" default="false"/>
								<xs:attribute name="PUBLISH_GIOVANNI" type="xs:boolean" use="optional" default="false"/>
								<xs:attribute name="EXPIRY" use="optional">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:pattern value="\+?[1-9][0-9]*"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="DOC" type="xs:string" use="optional"/>
								<xs:attribute name="WITH_HDF4MAP" type="xs:boolean" use="optional" default="false"/>
								<xs:attribute name="REPLICATE" type="xs:boolean" use="optional" default="false"/>
								<xs:attribute name="MULTIPLE_REPLACEMENT" use="optional">
									<xs:annotation>
										<xs:documentation>Default action when the incoming granule trigger multiple deletion.</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="STORE"/>
											<xs:enumeration value="SKIP"/>
											<xs:enumeration value=""/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="COLLECTION_LINK" use="optional" default="S4PA">
									<xs:annotation>
										<xs:documentation>Collection metadata link in granule metadata file.</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="S4PA"/>
											<xs:enumeration value="CMR"/>
											<xs:enumeration value=""/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="NAME" use="required">
						<xs:annotation>
							<xs:documentation>Dataset name. Must be alpha-numerical.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="([a-z]|[A-Z]|[0-9]|_|-){1,30}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="GROUP" use="optional">
						<xs:annotation>
							<xs:documentation>Data group name. Must be alpha-numerical.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
