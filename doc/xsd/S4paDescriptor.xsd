					<xs:attribute name="PUBLISH_WHOM" type="xs:boolean" use="optional" default="false"/>
					<xs:attribute name="PUBLISH_ECHO" type="xs:boolean" use="optional" default="false"/>
					<xs:attribute name="PUBLISH_ECHO_OPENDAP" type="xs:boolean" use="optional"/>
					<xs:attribute name="PUBLISH_CMR_OPENDAP" type="xs:boolean" use="optional"/>
					<xs:attribute name="OPENDAP_URL_PREFIX" type="xs:string" use="optional"/>
					<xs:attribute name="OPENDAP_URL_SUFFIX" type="xs:string" use="optional"/>
					<xs:attribute name="OPENDAP_RESOURCE_URL_SUFFIX" type="xs:string" use="optional"/>
					<xs:attribute name="PUBLISH_CMR" type="xs:boolean" use="optional" default="true"/>
					<xs:attribute name="PUBLISH_MIRADOR" type="xs:boolean" use="optional" default="false"/>
					<xs:attribute name="PUBLISH_GIOVANNI" type="xs:boolean" use="optional" default="false"/>
					<xs:attribute name="EXPIRY" use="optional">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\+?[1-9][0-9]*"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="DOC" type="xs:string" use="optional"/>
					<xs:attribute name="WITH_HDF4MAP" type="xs:boolean" use="optional" default="false"/>
					<xs:attribute name="REPLICATE" type="xs:boolean" use="optional" default="false"/>
					<xs:attribute name="MULTIPLE_REPLACEMENT" use="optional">
						<xs:annotation>
							<xs:documentation>Default action when the incoming granule trigger multiple deletion.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="STORE"/>
								<xs:enumeration value="SKIP"/>
								<xs:enumeration value=""/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="COLLECTION_LINK" use="optional" default="S4PA">
						<xs:annotation>
							<xs:documentation>Collection metadata link in granule metadata file.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="S4PA"/>
								<xs:enumeration value="CMR"/>
								<xs:enumeration value=""/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="NAME" use="required">
			<xs:annotation>
				<xs:documentation>Name of the data class (logical grouping of datasets internal to S4PA).</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="([a-z]|[A-Z]|[0-9]|_)+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="GROUP" use="required">
