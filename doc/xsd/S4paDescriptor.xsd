				<xs:documentation>Name of the data group (logical grouping of datasets from data user's perspective).</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="([a-z]|[A-Z]|[0-9]|_)+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FREQUENCY" use="optional" default="daily">
